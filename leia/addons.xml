<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.milhanothemes" name="Milhano Themes" version="2.1" provider-name="Milhano Themes">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Milhano Themes</summary>
    <description lang="en">Welcome to the Milhano Themes where you can get all the latest Milhano Custom Themes for kodi 17,18</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.f4mTester" version="3.0.1.1" name="f4mTester" provider-name="Shani">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.video.F4mProxy" version="3.0.1.1"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>


    <extension point="xbmc.addon.metadata">
        <summary>f4mTester</summary>
        <description></description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="script.video.F4mProxy"
	     name="F4mProxy"
	     version="2.8.7"
	     provider-name="Shani">
  <requires>
    <import addon="xbmc.python" version="2.1.0" />
    <import addon="script.module.requests" />
  </requires>
  <extension point="xbmc.python.module" library="lib" />
    <extension point="xbmc.python.pluginsource" library="addon.py">
		<provides>executable</provides>
  </extension>



  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Proxy for f4m</summary>
    <description lang="en">Use this addon to play F4m v1 and v2 streams</description>
  </extension>
</addon>

<addon id="service.subtitles.opensubtitles.free" name="OpenSubtitles Free" provider-name="amet and Milhano" version="0.0.3">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension library="service.py" point="xbmc.subtitle.module"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Retrieved from version 5.0.16 OpenSubtitles.</summary>
	<summary lang="pt">Retirado da versao 5.0.16 OpenSubtitles.</summary>
    <description lang="en">Search and download subtitles on opensubtitles.org without resistance.</description>
	<description lang="pt">Pesquise e baixe legendas em opensubtitles.org sem resisto.</description>
    <disclaimer lang="en"/>
    <platform>all</platform>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum></forum>
    <website/>
    <email/>
    <source></source>
  </extension>
</addon>
</addons>
